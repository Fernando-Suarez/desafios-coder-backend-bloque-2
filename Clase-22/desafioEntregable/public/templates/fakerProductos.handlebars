<div class='container'>
	<table class='content-table'>
		<thead>
			<tr>
				<th>Nombre</th>
				<th>Precio</th>
				<th>Categoria</th>
				<th>Foto</th>
			</tr>
		</thead>
		<tbody>
			{{#each products}}
				<tr>
					<td>{{this.nombre}}</td>
					<td>{{this.precio}}</td>
					<td>{{this.categoria}}</td>
					<td><img width='60' src={{this.thumbnail}} /></td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>